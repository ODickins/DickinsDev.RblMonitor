@model DickinsDev.RblMonitor.WebUI.Models.Home.QuickCheckModel
@*
    For more information on enabling MVC for empty projects, visit https://go.microsoft.com/fwlink/?LinkID=397860
*@
@{
}
<h1>@Model.IPAddress</h1>
@switch (Model.isClean)
{
    case true:
        <p>IP Address is clean.</p>
        break;
    case false:
        <p>IP Address has been found on 1 or more blacklists.</p>
        break;
}
@foreach (var dn in Model.DnsCheckResult.RBLChecks)
{
    <ul>
        <li>Clean: @dn.isClean</li>
        <li>Zone Name: @dn.RBL.ZoneName</li>
        <li>Response: @dn.Response</li>
        <li>Error: @dn.Error</li>
    </ul>
}